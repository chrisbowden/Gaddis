# This program demonstrates the Canvas Widget
import tkinter

class MyGUI:
    def __init__(self):
        # Create main window
        self.main_window = tkinter.Tk()

        # Create the Canvas widget
        self.canvas = tkinter.Canvas(self.main_window, width=200, height=200)

        # Draw an Arc
        self.canvas.create_arc(10,10,190,190, start=45, extent=30)
        
        # Pack the canvas
        self.canvas.pack()

        # Main loop
        tkinter.mainloop()

# Create object
my_gui = MyGUI()

        
